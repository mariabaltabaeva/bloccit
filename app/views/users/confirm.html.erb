<h4>Sign Up Confirmation</h4>
<div class="row">
  <div class="col-md-8">
<h4>Are you sure these values are correct?</h4>
  <h5>Email: <%= @user.email %></h5>
  <h5>Name: <%= @user.name %></h5>
</div>
</div>
<div class="row">
  <div class="col-md-8">
<%= button_to "Yes", { controller: "users", action: "create", params: { user: user_params } }, class: 'btn btn-success' %>
<%= link_to  "No", new_user_path, class: 'btn btn-danger'%>

</div>
</div>
